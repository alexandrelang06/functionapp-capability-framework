{
  "routes": [
    {
      "route": "/api/auth/{*rest}",
      "allowedMethods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"],
      "rewrite": "/api/auth/{rest}"
    },
    {
      "route": "/api/rest/{*rest}",
      "allowedMethods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"],
      "rewrite": "/api/rest/{rest}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  }
}
