{
  "routes": [
    {
      "route": "/api/auth/*",
      "rewrite": "https://it-framework-function-app.azurewebsites.net/api/auth/*"
    },
    {
      "route": "/api/rest/*",
      "rewrite": "https://it-framework-function-app.azurewebsites.net/api/rest/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/api/*" ]
  }
}
